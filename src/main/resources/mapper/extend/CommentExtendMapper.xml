<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.evan.jz.dao.extend.CommentExtendMapper">
  
  <select id="selectByArticleId" resultMap="com.evan.jz.dao.CommentMapper.BaseResultMap">
  	select * 
  	from cms_comment
  	where article_id = #{articleId}
  </select>
</mapper>